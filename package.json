{
	"name": "vsls-communities",
	"displayName": "vsls-communities",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vsls-communities.joinCommunity",
				"category": "Live Share Communities",
				"title": "Join Community"
			},
			{
				"command": "vsls-communities.leaveCommunity",
				"category": "Live Share Communities",
				"title": "Leave Community"
			},
			{
				"command": "vsls-communities.refresh",
				"category": "Live Share Communities",
				"title": "Refresh Communities"
			}
		],
		"views": {
			"liveshare": [
				{
					"id": "vsls-communities.communities",
					"name": "Communities"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "Live Share Communities",
			"properties": {
				"vsls-communities.showSuggestedContacts": {
					"default": true,
					"type": "boolean",
					"description": "Specifies whether to show community contacts in the Live Share Suggested Contacts tree."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/ramda": "^0.26.18",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.0-next.0"
	},
	"dependencies": {
		"ramda": "^0.26.1",
		"redux": "^4.0.4",
		"redux-thunk": "^2.3.0",
		"vsls": "^0.3.1291"
	}
}
